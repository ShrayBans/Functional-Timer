{
   "name": "Functional Pomodoro Timer",
   "description": "25 Minute Timer that closes distracting web pages when timer is up.",
   "version": "1.0",
   "manifest_version": 2,

   "content_scripts": [
     {
       "matches": ["<all_urls>"],
       "js": ["jquery-2.2.4.min.js", "extension.js"]
     }
   ],

   "background": {
     "scripts": [
       "background.js"
     ]
   },

   "browser_action": {
      "default_icon": "icon.png",
      "default_title": "Functional Pomodoro Timer",
      "default_popup":"popup.html"
      },

     "permissions": [ "tabs", "storage", "webRequest", "background", "webRequestBlocking" ]


}
